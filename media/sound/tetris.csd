<CsoundSynthesizer><CsInstruments>; tetris clicks, based on csound tutorialsr        =         44100kr        =         4410ksmps     =         10nchnls    =         1 instr	3	idur		= p3	iamp		= ampdb(p4)	ifreq		= cpspch(p5)	iattac	= p6	irelease	= p7		k1	linen	iamp, iattac, idur, irelease	a1	oscil	k1, ifreq, 1		out	a1endin	instr	4	iwhite    =         22050	idur      =         p3	iamp      =         p4	iswpstart =         p5	isweepend =         p6	ibndwidth =         p7	iattack	=	  p8	irelease	=	  p9	kamp      linen     iamp, iattack, idur, irelease	ksweep    line      iswpstart, idur, isweepend	asig      rand      iwhite	afilt     reson     asig, ksweep, ibndwidth	arampsig  =         kamp * afilt          out      arampsigendininstr 5	idur	= p3	iamp	= ampdb(p4)	iadep	= ampdb(p5) ; tremolo depth	iafreq= p6	; tremolo freq	ifreq	= cpspch(p7) 	ifdep	= p8 ; vibrato depth	iffreq = p9 ; vibrato freq	iattac	= p10	irelease	= p11	k1	linen	iamp,	iattac, idur, irelease	k2	oscil iadep, iafreq, 1	k3	linen	1, iattac, idur, irelease	k3 	= k1 + k2*k3	k4	oscil ifdep, iffreq, 1	k4	= ifreq + k4	a1	oscil	k3,	k4,	1		out	a1endininstr 1  ifr  = cpspch(p4)  iamp = ampdb(p5)  iat  = p6  irel = p7  aenv expsega 0.01, iat, iamp, irel, 0.01  a1 oscil aenv, ifr, 1  a2 oscil aenv, ifr*1.2, 1  a3 oscil aenv, ifr*1.4, 1  aut = a1 + a2 + a3  out autendin</CsInstruments><CsScore>f1	0	4096	10	1	;	sine	wave;	click;i1	0	.3	8.10	75	0.05	0.5;i3	0	0.3	80	9.00	0	0.1;s;	slide;i4	0 	.3 .006 2100	500	50	.01	.1;s; snapi1	0	.1	9.00	85	.01	.1i1	+	.2	8.10	80	.01	.1s;	trrrr;i5	0	0.5	80	70	20	8.10	200	20	0	0.3;se</CsScore></CsoundSynthesizer><MacOptions>Version: 3Render: FileAsk: YesFunctions: NoneListing: ioObjectWindowBounds: 204 148 885 719CurrentView: ioIOViewEdit: OffOptions: -b128 -W -o/Users/artm/Music/csound/my/tetris-snap.wav -s -m167 -R --midi-velocity-amp=4 --midi-key-cps=5 </MacOptions><MacGU